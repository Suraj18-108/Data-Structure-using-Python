    "import numpy as np\n",
    "y=np.array([55,69,22,420,253])\n",
    "def average(x):\n",
    "    sum_a = np.sum(x)\n",
    "    len_a = len(x)\n",
    "    avg_a = sum_a/len_a\n",
    "    return avg_a,sum_a\n",
    "\n",
    "result = average(y)\n",
    "print(result)"
